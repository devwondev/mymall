<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.test.mymall.dao.MemberMapper">
  <select id="selectMemberList" parameterType="String" resultType="com.test.mymall.vo.Member">
    SELECT 
      no as no
    FROM member
  </select>
  <select id="selectMemberOne" parameterType="int">
    SELECT 
      no as no
    FROM member
    WHERE no=#{}
  </select>
  <select id="insertMember" parameterType="com.test.mymall.vo.Member">
    INSERT INTO member
      (no)
    VALUES #{no}
  </select>
</mapper>